{
  "name": "html-handler",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "pretsc": "rimraf dest",
    "start": "npm run tsc && node dest/index.js",
    "tsc": "tsc -p .",
    "prettier": "prettier --write \"**/*.{md,js,ts,json}\" && git add . && git status",
    "test": "jest --runInBand --coverage",
    "prepublishOnly": "npm run prettier && npm run test && npm run tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rchaser53/html-handler.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rchaser53/html-handler/issues"
  },
  "homepage": "https://github.com/rchaser53/html-handler#readme",
  "dependencies": {
    "parse5": "^4.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "<rootDir>/preprocessor.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "dest"
    ],
    "testMatch": [
      "**/__tests__/*.(ts|js)"
    ]
  },
  "devDependencies": {
    "@types/jest": "^22.1.2",
    "@types/node": "^8.9.1",
    "express": "^4.16.2",
    "husky": "^0.14.3",
    "jest": "^22.2.1",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  }
}
